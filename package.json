{
  "name": "overseer",
  "version": "0.1.9",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "vite-dev": "vite",
    "vite-build": "tsc && vite build",
    "vite-preview": "vite preview",
    "tauri": "tauri",
    "dev": "tauri dev",
    "build": "tauri build --bundles app",
    "build:linux": "tauri build --bundles appimage,deb",
    "start": "pnpm build && pnpm open",
    "open": "open src-tauri/target/release/bundle/macos/Overseer.app",
    "format": "prettier --write src --experimental-cli",
    "format:check": "prettier --check src --experimental-cli",
    "lint": "oxlint src/",
    "typecheck": "tsgo --noEmit",
    "rustcheck": "cargo check --manifest-path src-tauri/Cargo.toml",
    "checks": "pnpm run \"/format:check|lint|typecheck|rustcheck/\"",
    "checks:ui": "pnpm run \"/format:check|lint|typecheck/\" ",
    "test": "vitest run --silent --reporter=dot",
    "test:watch": "vitest --silent --reporter=dot",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "packageManager": "pnpm@10.17.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.10.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/diff": "^8.0.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript/native-preview": "7.0.0-dev.20260205.1",
    "@vitejs/plugin-react": "^5.1.2",
    "jsdom": "^28.0.0",
    "oxlint": "^1.43.0",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@faker-js/faker": "^10.3.0",
    "@opencode-ai/sdk": "^1.1.53",
    "@pierre/diffs": "^1.0.10",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-os": "^2.3.0",
    "@tauri-apps/plugin-process": "~2.3.0",
    "@tauri-apps/plugin-shell": "^2.3.4",
    "@tauri-apps/plugin-updater": "~2.10.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "classnames": "^2.5.1",
    "diff": "^8.0.3",
    "lucide-react": "^0.563.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "xterm": "^5.3.0"
  }
}
