{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Overseer",
  "version": "0.1.1",
  "identifier": "com.overseer.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "pnpm vite-dev",
    "beforeBuildCommand": "pnpm vite-build"
  },
  "app": {
    "windows": [
      {
        "title": "",
        "width": 1200,
        "height": 800,
        "minWidth": 900,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "maximized": true,
        "titleBarStyle": "Overlay",
        "visible": false,
        "backgroundColor": "#1b1f24ff"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
